<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <queries>
        <package android:name="com.google.earth" />
    </queries>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/splashnew"
        android:label="@string/app_name"
        android:roundIcon="@drawable/splashnew"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidProject">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyByM1Ph4vePwdW94H1uHriQHrn1bzC-Qmo" />

        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTop" />
        <activity android:name=".PlacesActivity" />
    </application>

</manifest>